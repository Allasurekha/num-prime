#include <stdio.h>
int prime(int num);

int main()
{
    int num,check;
    printf("enter num");
    scanf("%d",&num);
    check=prime(num);
    if(check==0)
    printf("%d is prime",num);
    else
    printf("%d is not prime" ,num);
    }
int prime(int num)
{
    int i,check;
    check=0;
    for(i=2;i<num;i++)
    {
        if(num%i==0)
        check++;
            }
            return check;
}
   
    
